{
  "name": "@dota2wiki/database",
  "version": "1.0.0",
  "description": "DOTA2 built-in data for dota2wiki.",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "repository": {
    "type": "github",
    "url": "https://github.com/dota2wiki/dota2wiki"
  },
  "keywords": [
    "dota2"
  ],
  "author": "duduluu <duduluu@outlook.com>",
  "license": "MIT",
  "scripts": {
